<header>
	<div class="container">
		<div class="g4">
			<h1><a href="/index"><span>WS</span> Apps</a></h1>
		</div>
		<div class="g8">
			<nav>
				<ul>
					<% for(var nav in navItems){
						if (nav !== "index"){
							if (current.path.length < 2) { %>
								<li class="<%- (current.source == nav) ? 'active' : '' %>"><a href="<%- (nav == 'support') ? 'http://app-help.winespectator.com/' : '../'+nav %>" target="<%- (nav == 'support') ? '_blank' : '' %>"><%-: nav === 'wineratingsplus' ? 'wineratings+' : nav | capitalize %></a></li>
							<% } else { %>
								<li class="<%- (current.path[1] == nav) ? 'active' : '' %>"><a href="<%- (nav == 'support') ? 'http://app-help.winespectator.com/' : '../../'+nav %>" target="<%- (nav == 'support') ? '_blank' : '' %>"><%-: nav === 'wineratingsplus' ? 'wineratings+' : nav | capitalize %></a></li>
							<% };
						};
					}; %>
					<!-- <li><a href="/wineratingsplus/" title="WineRatings+">WineRatings+</a></li>
					<li><a href="/xvalues/" title="XValues">Xvalues</a></li>
					<li><a href="http://app-help.winespectator.com/">Support</a></li>
					<li><a href="/vintagechartplus" title="VintageChart+">VintageChart+</a></li>
					<li><a href="/guide-to-napa-valley" title="Guide to Napa Valley">Guide to Napa Valley</a></li> -->
				</ul>
			</nav>
		</div>
	</div><!--/container-->
</header>